{
  "annotator": {
    "autosave": false,
    "default_animals": ["R", "G", "B", "Y"],
    "default_fps": 30,
    "default_types": [
      {
        "color": "#4f8cff",
        "key": "g",
        "mode": "single",
        "name": "Grooming"
      },
      {
        "color": "#ff6b6b",
        "key": "c",
        "mode": "agonistic",
        "name": "Chasing"
      },
      {
        "color": "#ffd166",
        "key": "s",
        "mode": "single",
        "name": "Sniffing"
      }
    ],
    "keyboard": {
      "frame_step_keys": {
        "next": "]",
        "prev": "["
      },
      "jump_seconds": {
        "alt": 10,
        "left": 300,
        "right": 300,
        "shift": 60
      }
    }
  },
  "browser": {
    "preview_thumbnails": 8,
    "visible_extensions": [".mp4", ".avi"]
  },
  "facilities": {
    "chick": {
      "camera_pattern": "cam{cam:02d}",
      "source_dir": "/mnt/nas/chick",
      "ignore_dir_regex": "^@",
      "max_file_duration": "4:00",
      "output_dir": "/tmp/cheesepie-out",
      "path_time_regex": "(?P<year>\\d{4})(?P<month>\\d{2})(?P<day>\\d{2})[_-](?P<hour>\\d{2})(?P<minute>\\d{2})(?:(?P<second>\\d{2}))?",
      "camera_list": [21, 22, 23, 24, 25],
      "experiments": {
        "OPEN_FIELD": ["wt"],
        "PECKING_ORDER": [
          {
            "end_time": "12:00",
            "name": "pilot",
            "start_time": "08:00"
          },
          "wt"
        ]
      },
      "setups": {
        "default": {
          "arena_width_cm": 60,
          "arena_height_cm": 60,
          "grid_cols": 6,
          "grid_rows": 6,
          "bg_frames": 20,
          "bg_quantile": 50,
          "arena": { "x": 150, "y": 100, "width": 800, "height": 800 },
          "roi": {
            "Feeder1": { "cells": [[4, 0]], "sheltered": false },
            "Feeder2": { "cells": [[4, 5]], "sheltered": false },
            "Water1": { "cells": [[5, 0]], "sheltered": false },
            "Water2": { "cells": [[5, 5]], "sheltered": false },
            "Nest": {
              "cells": [
                [0, 2],
                [0, 3],
                [1, 2],
                [1, 3]
              ],
              "sheltered": true
            }
          }
        }
      }
    },
    "mouse": {
      "ignore_dir_regex": "^@",
      "max_file_duration": "4:00",
      "output_dir": "/tmp/cheesepie-out",
      "path_time_regex": "(?P<year>\\d{4})(?P<month>\\d{2})(?P<day>\\d{2}).*?-(?P<hour>\\d{2})(?P<minute>\\d{2})(?P<second>\\d{2})",
      "source_dir": "/Volumes/surveillance/",
      "camera_list": [1, 2, 3, 4, 5, 6, 7, 8],
      "camera_pattern": "CHEESE{cam:02d}",
      "experiments": {
        "FEMALES_IDS": [
          {
            "end_time": "08:00",
            "name": "wt",
            "start_time": "20:00"
          },
          "overnight"
        ],
        "SHELTER_SEEKING": [
          "pilot",
          {
            "end_time": "14:00",
            "name": "wt",
            "start_time": "10:00"
          }
        ],
        "THREE_CHAMBERS": [
          {
            "end_time": "08:00",
            "name": "wt",
            "start_time": "20:00"
          },
          {
            "end_time": "17:00",
            "name": "pilot",
            "start_time": "09:00"
          },
          {
            "end_time": "08:00",
            "name": "overnight",
            "start_time": "20:00"
          }
        ]
      },
      "setups": {
        "default": {
          "arena_width_cm": 60,
          "arena_height_cm": 60,
          "grid_cols": 6,
          "grid_rows": 6,
          "bg_frames": 20,
          "bg_quantile": 50,
          "arena": { "x": 150, "y": 100, "width": 800, "height": 800 },
          "roi": {
            "Feeder1": { "cells": [[4, 0]], "sheltered": false },
            "Feeder2": { "cells": [[4, 5]], "sheltered": false },
            "Water1": { "cells": [[5, 0]], "sheltered": false },
            "Water2": { "cells": [[5, 5]], "sheltered": false },
            "Nest": {
              "cells": [
                [0, 2],
                [0, 3],
                [1, 2],
                [1, 3]
              ],
              "sheltered": true
            }
          }
        }
      }
    }
  },
  "importer": {
    "ignore_dir_regex": "^@",
    "source_extensions": [".mp4", ".mkv", ".avi"],
    "working_dir": "./working"
  },
  "matlab": {
    "binary": "/Applications/MATLAB_R2025a.app/bin/matlab",
    "enabled": true,
    "mode": "engine",
    "paths": ["./matlab"],
    "whitelist": ["segment_frame"]
  }
}
